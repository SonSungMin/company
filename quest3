    public void SetAutoComplete(bool auto)
    {
        ((LookUpEditBase)this.AmiComboBoxEdit).AutoComplete = auto;
    }

    private void Ok_Click(object sender, RoutedEventArgs e)
    {
        this.IsOK = true;
        if (((LookUpEditBase)this.AmiComboBoxEdit).SelectedItem != null)
            this._SelectedItem = ((LookUpEditBase)this.AmiComboBoxEdit).SelectedItem;
        ((Window)this).Close();
    }
    private void Cancel_Click(object sender, RoutedEventArgs e)
    {
        this.IsOK = false;
        ((Window)this).Close();
    }

