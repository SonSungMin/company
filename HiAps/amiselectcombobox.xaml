<core:DXWindow
	x:Class="Amisys.Framework.Infrastructure.UserControls.AmiSelectComboBox" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
	UIElement.SnapsToDevicePixels="True"
	FrameworkElement.UseLayoutRounding="True"
	ThemeManager.Theme="DXStyle"
	Window.Title="Select Item"
	FrameworkElement.Height="109"
	FrameworkElement.Width="213" xmlns:usercontrols="clr-namespace:Amisys.Framework.Infrastructure.UserControls" xmlns:core="clr-namespace:DevExpress.Xpf.Core;assembly=DevExpress.Xpf.Core.v13.2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition
				Height="30" />
            <RowDefinition
				Height="5" />
            <RowDefinition
				Height="35" />
        </Grid.RowDefinitions>
        <editors:ComboBoxEdit
			ComboBoxEdit.Name="AmiComboBoxEdit"
			Grid.Row="0"
			FrameworkElement.VerticalAlignment="Stretch"
			FrameworkElement.HorizontalAlignment="Stretch"
			LookUpEditBase.AutoComplete="True"
			FrameworkElement.Margin="1,0,1,0" xmlns:editors="clr-namespace:DevExpress.Xpf.Editors;assembly=DevExpress.Xpf.Core.v13.2"
			x:Name="AmiComboBoxEdit" />
        <StackPanel
			Grid.Row="2"
			Orientation="Horizontal"
			Margin="1">
            <Button
				Name="Cancel"
				Height="28"
				Width="90"
				Margin="3,0,0,0"
				Click="Cancel_Click">
                <StackPanel
					Orientation="Horizontal">
                    <Image
						Source="pack://application:,,,/AmiResources;component/Images/Icons/AmiCancel.ico"
						Width="22" />
                    <TextBlock
						Text="Cancel"
						Margin="3,0,0,0"
						FontWeight="Black"
						VerticalAlignment="Center" />
                </StackPanel>
            </Button>
            <Button
				Name="Ok"
				Width="90"
				Height="28"
				Margin="3,0,0,0"
				Click="Ok_Click">
                <StackPanel
					Orientation="Horizontal">
                    <Image
						Source="pack://application:,,,/AmiResources;component/Images/Icons/AmiSelect.ico"
						Width="22" />
                    <TextBlock
						Text="Select"
						Margin="3,0,0,0"
						FontWeight="Black"
						VerticalAlignment="Center" />
                </StackPanel>
            </Button>
        </StackPanel>
    </Grid>
</core:DXWindow>



